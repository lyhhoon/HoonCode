	document.write('더하기 : ' + numResult + '<br/>');
	
	numResult = num1 - num2;
	document.write('빼기 : ' + numResult + '<br/>');
	
	numResult = num1 * num2;
	document.write('곱하기 : ' + numResult + '<br/>');
	
	numResult = num1 / num2;
	document.write('나누기 : ' + numResult + '<br/>');
	
	numResult = num1 % num2;
	document.write('나머지 : ' + numResult);
	
</script>

